<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="apMain" prefHeight="336.0" prefWidth="524.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <Label layoutX="74.0" layoutY="84.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="76.0" layoutY="132.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="bLogin" layoutX="127.0" layoutY="242.0" mnemonicParsing="false" onAction="#checkUserAndPass" text="Login" />
      <Button fx:id="bRegister" layoutX="354.0" layoutY="242.0" mnemonicParsing="false" onAction="#goRegister" text="Register" />
      <TextField fx:id="tfUserName" layoutX="150.0" layoutY="80.0" prefHeight="25.0" prefWidth="234.0" />
      <PasswordField fx:id="pfLPassword" layoutX="150.0" layoutY="128.0" prefHeight="25.0" prefWidth="234.0" />
   </children>
</AnchorPane>
